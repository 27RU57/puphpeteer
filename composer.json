{
    "name": "extractr-io/puphpeteer",
    "description": "A Puppeteer bridge for PHP, supporting the full API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Pardanaud",
            "email": "pardanaud.j@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "extractr-io/rialto": "dev-master"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.5.4",
        "phpunit/phpunit": "^6.5",
        "symfony/process": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ExtractrIo\\Puphpeteer\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "npm install",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
